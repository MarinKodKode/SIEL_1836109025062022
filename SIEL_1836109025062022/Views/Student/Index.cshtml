@using Microsoft.AspNetCore.Identity
@model StudenIndexViewModel


@{
    ViewData["Title"] = "English Discoveries";
    var status = int.Parse(ViewData["studentProgram"].ToString());   
      
}


@if(status == 0)
{
    <partial name = "_NoProgramSelected" />
}
else
{
    <partial name = "_StudentProgramList"/>
}


